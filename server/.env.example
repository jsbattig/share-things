# Server Configuration
PORT=3001
NODE_ENV=production

# CORS Configuration
# Replace with your frontend URL (including port if using custom ports)
CORS_ORIGIN=http://localhost

# Session Configuration
SESSION_TIMEOUT=600000  # 10 minutes in milliseconds
SESSION_EXPIRY=86400000  # 24 hours in milliseconds

# Logging Configuration
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW=60000  # 1 minute in milliseconds
RATE_LIMIT_MAX=100       # Maximum 100 requests per window